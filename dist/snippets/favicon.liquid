<link rel="apple-touch-icon" sizes="180x180" href="{{ settings.apple_touch_icon | image_url: width: 180, height: 180 }}">
<link rel="icon" type="image/png" sizes="32x32" href="{{ settings.favicon_png_32 | image_url: width: 32, height: 32 }}">
<link rel="icon" type="image/png" sizes="16x16" href="{{ settings.favicon_png_16 | image_url: width: 16, height: 16 }}">
<meta name="msapplication-TileColor" content="{{ settings.color_windows_tile }}">
<meta name="theme-color" content="{{ settings.color_android_theme }}">
